<?php

/**
 * Implements hook_preprocess_node().
 */
function custom_page_preprocess_node(&$variables) {
  // Check if the current page is custom page.
  $route_name = \Drupal::routeMatch()->getRouteName();
  if ($route_name == 'custom_page.example') {
    $variables['#attached']['library'][] = 'custom_page/custom_page_css';
  }
}
